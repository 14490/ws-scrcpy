{
  "name": "ws-scrcpy",
  "version": "0.0.1",
  "description": "ws client for scrcpy",
  "main": "index.js",
  "scripts": {
    "compile": "./node_modules/.bin/tsc -p .",
    "build": "npm run compile && cp -r vendor/* build && ./node_modules/.bin/browserify build/index.js -o dist/bundle.js && cp public/* dist",
    "start": "npm run build && ./node_modules/.bin/http-server ./dist -o",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "Sergey Volkov <s.volkov@netris.ru>",
  "license": "MIT",
  "dependencies": {
    "buffer": "^5.2.1",
    "h264-converter": "^0.1.0",
    "sylvester.js": "^0.1.1"
  },
  "devDependencies": {
    "@types/node": "^12.0.2",
    "browserify": "^16.2.3",
    "http-server": "^0.11.1",
    "tslib": "^1.9.3",
    "typescript": "^3.4.5"
  }
}
